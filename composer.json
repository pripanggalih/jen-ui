        "laravel/prompts": "^0|^1",
        "blade-ui-kit/blade-heroicons": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10|^11",
        "orchestra/testbench": "^8|^9|^10"
    },
    "autoload": {
        "psr-4": {
