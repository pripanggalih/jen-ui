    "keywords": [
        "laravel",
        "livewire",
        "livewire 3",
        "livewire ui",
        "livewire ui components",
        "tallstack",
        "tallstack ui",
        "tallstackui",
        "tallstack components",
        "blade",
        "ui",
        "blade ui components",
        "livewire-components",
        "livewire components",
        "livewire-packages",
        "livewire packages",
        "tailwind",
        "components",
        "daisyUI",
        "alpinejs"
